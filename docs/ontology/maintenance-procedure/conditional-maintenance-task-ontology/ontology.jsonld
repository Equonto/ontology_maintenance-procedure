}, {
  "@id" : "http://spec.equonto.org/ontology/maintenance-procedure/conditional-maintenance-task-ontology",
  "@type" : [ "http://www.w3.org/2002/07/owl#Ontology" ],
  "http://purl.org/dc/terms/abstract" : [ {
    "@value" : "The OMPD Conditional Maintenance Task Ontology captures functional failures and corrective maintenance tasks (to be conditionally executed on the occurrence of a function failure) found in some industrial maintenance procedures. This ontology captures information currently stored in maintenance procedure documentation in a generalizable way and supports its core users (maintenance technicians, maintenance engineers and maintenance schedulers) in their work. This ontology is designed to be used alongside the Static Procedure Ontology (https://spec.equonto.org/ontology/maintenance-procedure/static-procedure-ontology/) for a more detailed description of maintenance procedures. Users can find details of the design methodology for this ontology at https://content.iospress.com/articles/applied-ontology/ao230279"
  } ],
  "http://purl.org/dc/terms/contributor" : [ {
    "@value" : "Caitlin Woods: The University of Western Australia and Equonto"
  }, {
    "@value" : "Melinda Hodkiewicz: The University of Western Australia"
  }, {
    "@value" : "Tim French: The University of Western Australia"
  } ],
  "http://purl.org/dc/terms/creator" : [ {
    "@value" : "The University of Western Australia & Equonto Pty. Ltd."
  } ],
  "http://purl.org/dc/terms/license" : [ {
    "@value" : "http://opensource.org/licenses/MIT"
  } ],
  "http://purl.org/dc/terms/publisher" : [ {
    "@value" : "Equonto Pty. Ltd."
  } ],
  "http://purl.org/dc/terms/title" : [ {
    "@value" : "Ontology for Maintenance Procedure Documentation (OMPD) Conditional Maintenance Task Ontology"
  } ],
  "http://www.w3.org/2000/01/rdf-schema#label" : [ {
    "@value" : "Maintenance Procedures - Conditional Maintenance Task Ontology"
  } ],
  "http://www.w3.org/2002/07/owl#imports" : [ {
    "@id" : "http://spec.equonto.org/ontology/maintenance-procedure/static-procedure-ontology"
  } ],
