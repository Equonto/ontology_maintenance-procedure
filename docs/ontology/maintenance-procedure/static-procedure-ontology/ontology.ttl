@prefix : <http://spec.equonto.org/ontology/maintenance-procedure/static-procedure-ontology#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://spec.equonto.org/ontology/maintenance-procedure/static-procedure-ontology#> .

<http://spec.equonto.org/ontology/maintenance-procedure/static-procedure-ontology> rdf:type owl:Ontology ;
                                                                                    owl:versionIRI <http://spec.equonto.org/ontology/202312/maintenance-procedure/static-procedure-ontology> ;
                                                                                    owl:imports <http://rds.posccaesar.org/ontology/lis14/ont/core> ,
                                                                                                <https://spec.industrialontologies.org/ontology/core/meta/AnnotationVocabulary/> ;
                                                                                    <http://purl.org/dc/terms/abstract> "The OMPD Static Procedure Ontology captures static information found in most insdutrial maintenance procedures." ;
                                                                                    <http://purl.org/dc/terms/contributor> "Caitlin Woods: The University of Western Australia and Equonto" ,
                                                                                                                           "Melinda Hodkiewicz: The University of Western Australia" ,
                                                                                                                           "Tim French: The University of Western Australia" ;
                                                                                    <http://purl.org/dc/terms/creator> "The University of Western Australia & Equonto Pty. Ltd." ;
                                                                                    <http://purl.org/dc/terms/license> "http://opensource.org/licenses/MIT" ;
                                                                                    <http://purl.org/dc/terms/publisher> "Equonto Pty. Ltd." ;
                                                                                    <http://purl.org/dc/terms/title> "Ontology for Maintenance Procedure Documentation (OMPD) Static Procedure Ontology" ;
                                                                                    rdfs:label "Maintenance Procedures - Static Procedure Ontology" ;
                                                                                    <https://spec.industrialontologies.org/ontology/core/meta/AnnotationVocabulary/maturity> <https://spec.industrialontologies.org/ontology/core/meta/AnnotationVocabulary/Released> .

#################################################################
#    Annotation properties
#################################################################

###  http://swrl.stanford.edu/ontologies/3.3/swrla.owl#isRuleEnabled
<http://swrl.stanford.edu/ontologies/3.3/swrla.owl#isRuleEnabled> rdf:type owl:AnnotationProperty .


###  https://spec.industrialontologies.org/ontology/core/meta/AnnotationVocabulary/example
<https://spec.industrialontologies.org/ontology/core/meta/AnnotationVocabulary/example> rdf:type owl:AnnotationProperty .


###  https://spec.industrialontologies.org/ontology/core/meta/AnnotationVocabulary/semiFormalNaturalLanguageAxioms
<https://spec.industrialontologies.org/ontology/core/meta/AnnotationVocabulary/semiFormalNaturalLanguageAxioms> rdf:type owl:AnnotationProperty .


#################################################################
#    Datatypes
#################################################################

###  http://www.w3.org/2001/XMLSchema#string
xsd:string rdf:type rdfs:Datatype .


#################################################################
#    Object Properties
#################################################################

###  http://rds.posccaesar.org/ontology/lis14/rdl/concretizedBy
<http://rds.posccaesar.org/ontology/lis14/rdl/concretizedBy> rdf:type owl:ObjectProperty .


###  http://rds.posccaesar.org/ontology/lis14/rdl/hasActivityPart
<http://rds.posccaesar.org/ontology/lis14/rdl/hasActivityPart> rdf:type owl:ObjectProperty .


###  http://rds.posccaesar.org/ontology/lis14/rdl/hasQuality
<http://rds.posccaesar.org/ontology/lis14/rdl/hasQuality> rdf:type owl:ObjectProperty .


###  http://rds.posccaesar.org/ontology/lis14/rdl/hasRole
<http://rds.posccaesar.org/ontology/lis14/rdl/hasRole> rdf:type owl:ObjectProperty .


###  http://rds.posccaesar.org/ontology/lis14/rdl/isAbout
<http://rds.posccaesar.org/ontology/lis14/rdl/isAbout> rdf:type owl:ObjectProperty .


###  http://rds.posccaesar.org/ontology/lis14/rdl/occursRelativeTo
<http://rds.posccaesar.org/ontology/lis14/rdl/occursRelativeTo> rdf:type owl:ObjectProperty .


###  http://rds.posccaesar.org/ontology/lis14/rdl/realizedIn
<http://rds.posccaesar.org/ontology/lis14/rdl/realizedIn> rdf:type owl:ObjectProperty .


###  http://rds.posccaesar.org/ontology/lis14/rdl/representedBy
<http://rds.posccaesar.org/ontology/lis14/rdl/representedBy> rdf:type owl:ObjectProperty .


###  http://rds.posccaesar.org/ontology/lis14/rdl/roleOf
<http://rds.posccaesar.org/ontology/lis14/rdl/roleOf> rdf:type owl:ObjectProperty .


###  http://spec.equonto.org/ontology/maintenance-procedure/static-procedure-ontology#agentOf
:agentOf rdf:type owl:ObjectProperty ;
         rdfs:subPropertyOf <http://rds.posccaesar.org/ontology/lis14/rdl/participantIn> ;
         owl:inverseOf :hasAgent .


###  http://spec.equonto.org/ontology/maintenance-procedure/static-procedure-ontology#directActivityPartOf
:directActivityPartOf rdf:type owl:ObjectProperty ;
                      rdfs:subPropertyOf :indirectActivityPartOf ;
                      owl:inverseOf :hasDirectActivityPart ;
                      rdf:type owl:IrreflexiveProperty .


###  http://spec.equonto.org/ontology/maintenance-procedure/static-procedure-ontology#directlyAfter
:directlyAfter rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf :indirectlyAfter ;
               owl:inverseOf :directlyBefore .


###  http://spec.equonto.org/ontology/maintenance-procedure/static-procedure-ontology#directlyBefore
:directlyBefore rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf :indirectlyBefore ;
                rdf:type owl:FunctionalProperty ,
                         owl:IrreflexiveProperty .


###  http://spec.equonto.org/ontology/maintenance-procedure/static-procedure-ontology#hasAgent
:hasAgent rdf:type owl:ObjectProperty ;
          rdfs:subPropertyOf <http://rds.posccaesar.org/ontology/lis14/rdl/hasParticipant> .


###  http://spec.equonto.org/ontology/maintenance-procedure/static-procedure-ontology#hasDirectActivityPart
:hasDirectActivityPart rdf:type owl:ObjectProperty ;
                       rdfs:subPropertyOf :hasIndirectActivityPart .


###  http://spec.equonto.org/ontology/maintenance-procedure/static-procedure-ontology#hasIndirectActivityPart
:hasIndirectActivityPart rdf:type owl:ObjectProperty ;
                         rdfs:subPropertyOf <http://rds.posccaesar.org/ontology/lis14/rdl/hasActivityPart> ;
                         owl:inverseOf :indirectActivityPartOf ;
                         rdf:type owl:TransitiveProperty .


###  http://spec.equonto.org/ontology/maintenance-procedure/static-procedure-ontology#hasPatient
:hasPatient rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf <http://rds.posccaesar.org/ontology/lis14/rdl/hasParticipant> .


###  http://spec.equonto.org/ontology/maintenance-procedure/static-procedure-ontology#hasRequirement
:hasRequirement rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf <http://rds.posccaesar.org/ontology/lis14/rdl/hasParticipant> ;
                owl:inverseOf :requirementOf .


###  http://spec.equonto.org/ontology/maintenance-procedure/static-procedure-ontology#indirectActivityPartOf
:indirectActivityPartOf rdf:type owl:ObjectProperty ;
                        rdfs:subPropertyOf <http://rds.posccaesar.org/ontology/lis14/rdl/activityPartOf> ;
                        rdf:type owl:TransitiveProperty .


###  http://spec.equonto.org/ontology/maintenance-procedure/static-procedure-ontology#indirectlyAfter
:indirectlyAfter rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf <http://rds.posccaesar.org/ontology/lis14/rdl/after> ;
                 owl:inverseOf :indirectlyBefore .


###  http://spec.equonto.org/ontology/maintenance-procedure/static-procedure-ontology#indirectlyBefore
:indirectlyBefore rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf <http://rds.posccaesar.org/ontology/lis14/rdl/before> ;
                  rdf:type owl:TransitiveProperty .


###  http://spec.equonto.org/ontology/maintenance-procedure/static-procedure-ontology#locallyEquivalentTo
:locallyEquivalentTo rdf:type owl:ObjectProperty ;
                     rdfs:subPropertyOf :reasoningSupport ;
                     rdf:type owl:SymmetricProperty ;
                     rdfs:domain :MaintenanceTask ;
                     rdfs:range :MaintenanceTask .


###  http://spec.equonto.org/ontology/maintenance-procedure/static-procedure-ontology#patientOf
:patientOf rdf:type owl:ObjectProperty ;
           rdfs:subPropertyOf <http://rds.posccaesar.org/ontology/lis14/rdl/participantIn> ;
           owl:inverseOf :hasPatient .


###  http://spec.equonto.org/ontology/maintenance-procedure/static-procedure-ontology#reasoningSupport
:reasoningSupport rdf:type owl:ObjectProperty .


###  http://spec.equonto.org/ontology/maintenance-procedure/static-procedure-ontology#representedBy
:representedBy rdf:type owl:ObjectProperty .


###  http://spec.equonto.org/ontology/maintenance-procedure/static-procedure-ontology#requirementOf
:requirementOf rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf <http://rds.posccaesar.org/ontology/lis14/rdl/participantIn> .


#################################################################
#    Data properties
#################################################################

###  http://spec.equonto.org/ontology/maintenance-procedure/static-procedure-ontology#documentIdentifier
:documentIdentifier rdf:type owl:DatatypeProperty ;
                    rdfs:subPropertyOf owl:topDataProperty ;
                    rdfs:label "documentIdentifier" .


###  http://spec.equonto.org/ontology/maintenance-procedure/static-procedure-ontology#hasMediaLocation
:hasMediaLocation rdf:type owl:DatatypeProperty ;
                  rdfs:subPropertyOf owl:topDataProperty ;
                  rdfs:domain <http://rds.posccaesar.org/ontology/lis14/rdl/PhysicalObject> ;
                  rdfs:label "hasMediaLocation" .


###  http://spec.equonto.org/ontology/maintenance-procedure/static-procedure-ontology#hasTextValue
:hasTextValue rdf:type owl:DatatypeProperty ;
              rdfs:subPropertyOf owl:topDataProperty ;
              rdf:type owl:FunctionalProperty ;
              rdfs:label "hasTextValue" .


###  http://spec.equonto.org/ontology/maintenance-procedure/static-procedure-ontology#hasVersion
:hasVersion rdf:type owl:DatatypeProperty ;
            rdfs:subPropertyOf owl:topDataProperty ;
            rdf:type owl:FunctionalProperty ;
            rdfs:label "hasVersion" .


###  http://spec.equonto.org/ontology/maintenance-procedure/static-procedure-ontology#isInstructional
:isInstructional rdf:type owl:DatatypeProperty ;
                 rdfs:subPropertyOf owl:topDataProperty ;
                 rdf:type owl:FunctionalProperty ;
                 rdfs:domain :MaintenanceTaskDescription ;
                 rdfs:range xsd:boolean ;
                 rdfs:label "isInstructional" .


#################################################################
#    Classes
#################################################################

###  http://rds.posccaesar.org/ontology/lis14/rdl/Activity
<http://rds.posccaesar.org/ontology/lis14/rdl/Activity> rdf:type owl:Class .


###  http://rds.posccaesar.org/ontology/lis14/rdl/RealizableEntity
<http://rds.posccaesar.org/ontology/lis14/rdl/RealizableEntity> rdf:type owl:Class .


###  http://spec.equonto.org/ontology/maintenance-procedure/static-procedure-ontology#Animation
:Animation rdf:type owl:Class ;
           rdfs:subClassOf :MediaObject ;
           rdfs:comment "A LIS:MediaObject of type \"Animation\"" ;
           rdfs:label "Animation" ;
           <https://spec.industrialontologies.org/ontology/core/meta/AnnotationVocabulary/example> "an animated sequence describing which way to twist a bolt when torquing it (that can be accessed when viewing a procedure)" ;
           <https://spec.industrialontologies.org/ontology/core/meta/AnnotationVocabulary/isPrimitive> "true"^^xsd:boolean ;
           <https://spec.industrialontologies.org/ontology/core/meta/AnnotationVocabulary/maturity> <https://spec.industrialontologies.org/ontology/core/meta/AnnotationVocabulary/Released> ;
           <https://spec.industrialontologies.org/ontology/core/meta/AnnotationVocabulary/semiFormalNaturalLanguageAxioms> "A LIS:MediaObject of type \"Animation\"" .


###  http://spec.equonto.org/ontology/maintenance-procedure/static-procedure-ontology#AuxilliaryMaintenanceTaskDescription
:AuxilliaryMaintenanceTaskDescription rdf:type owl:Class ;
                                      owl:equivalentClass [ owl:intersectionOf ( :MaintenanceTaskDescription
                                                                                 [ rdf:type owl:Restriction ;
                                                                                   owl:onProperty :isInstructional ;
                                                                                   owl:hasValue "false"^^xsd:boolean
                                                                                 ]
                                                                               ) ;
                                                            rdf:type owl:Class
                                                          ] ;
                                      rdfs:subClassOf :MaintenanceTaskDescription ;
                                      rdfs:comment "A LIS:MaintenanceTaskDescription that provides  additional information for some agent to assist in the execution of a maintenance task." ;
                                      rdfs:label "AuxilliaryMaintenanceTaskDescription" ;
                                      <https://spec.industrialontologies.org/ontology/core/meta/AnnotationVocabulary/example> "\"hint: the engine has six bolts in total\"" ;
                                      <https://spec.industrialontologies.org/ontology/core/meta/AnnotationVocabulary/isPrimitive> "true"^^xsd:boolean ;
                                      <https://spec.industrialontologies.org/ontology/core/meta/AnnotationVocabulary/maturity> <https://spec.industrialontologies.org/ontology/core/meta/AnnotationVocabulary/Released> ;
                                      <https://spec.industrialontologies.org/ontology/core/meta/AnnotationVocabulary/semiFormalNaturalLanguageAxioms> "A LIS:MaintenanceTaskDescription that provides  additional information for some agent to assist in the execution of a maintenance task." ;
                                      <https://spec.industrialontologies.org/ontology/core/meta/AnnotationVocabulary/subjectMatterExpertExplanation> "the part of a document containing additional information (outside of the instructions) for performing a maintenance task" .


###  http://spec.equonto.org/ontology/maintenance-procedure/static-procedure-ontology#Component
:Component rdf:type owl:Class ;
           rdfs:subClassOf <http://rds.posccaesar.org/ontology/lis14/rdl/PhysicalObject> ;
           rdfs:comment "A LIS:PhysicalObject that may have some OMPD:ResourceRole" ;
           rdfs:label "Component" ;
           <https://spec.industrialontologies.org/ontology/core/meta/AnnotationVocabulary/example> "a circuit board, a pump shaft, a resistor, a cable" ;
           <https://spec.industrialontologies.org/ontology/core/meta/AnnotationVocabulary/isPrimitive> "true"^^xsd:boolean ;
           <https://spec.industrialontologies.org/ontology/core/meta/AnnotationVocabulary/maturity> <https://spec.industrialontologies.org/ontology/core/meta/AnnotationVocabulary/Released> ;
           <https://spec.industrialontologies.org/ontology/core/meta/AnnotationVocabulary/semiFormalNaturalLanguageAxioms> "A LIS:PhysicalObject that may have some OMPD:ResourceRole" ;
           <https://spec.industrialontologies.org/ontology/core/meta/AnnotationVocabulary/subjectMatterExpertExplanation> "an item of hardware, software or both that can be individually described and has a required function" .


###  http://spec.equonto.org/ontology/maintenance-procedure/static-procedure-ontology#ExecutableScript
:ExecutableScript rdf:type owl:Class ;
                  rdfs:subClassOf :MediaObject ;
                  rdfs:comment "A LIS:MediaObject of type \"ExecutableScript\"" ;
                  rdfs:label "ExecutableScript" ;
                  <https://spec.industrialontologies.org/ontology/core/meta/AnnotationVocabulary/example> "an assembly script sitting on a server for automating the movement of a robtic arm to unscrew a bolt." ;
                  <https://spec.industrialontologies.org/ontology/core/meta/AnnotationVocabulary/isPrimitive> "true"^^xsd:boolean ;
                  <https://spec.industrialontologies.org/ontology/core/meta/AnnotationVocabulary/maturity> <https://spec.industrialontologies.org/ontology/core/meta/AnnotationVocabulary/Released> ;
                  <https://spec.industrialontologies.org/ontology/core/meta/AnnotationVocabulary/semiFormalNaturalLanguageAxioms> "A LIS:MediaObject of type \"ExecutableScript\"" .


###  http://spec.equonto.org/ontology/maintenance-procedure/static-procedure-ontology#Hazard
:Hazard rdf:type owl:Class ;
        owl:equivalentClass [ owl:intersectionOf ( <http://rds.posccaesar.org/ontology/lis14/rdl/RealizableEntity>
                                                   [ rdf:type owl:Restriction ;
                                                     owl:onProperty <http://rds.posccaesar.org/ontology/lis14/rdl/realizedIn> ;
                                                     owl:someValuesFrom :HazardRealizationProcess
                                                   ]
                                                 ) ;
                              rdf:type owl:Class
                            ] ;
        rdfs:subClassOf <http://rds.posccaesar.org/ontology/lis14/rdl/Disposition> ;
        rdfs:comment "An LIS: Realizeable Entity that is realizrd in some OMPD: Hazard Realization Process" ;
        rdfs:label "Hazard" ;
        <https://spec.industrialontologies.org/ontology/core/meta/AnnotationVocabulary/example> "oil on the floor, a heavy object that needs to be moved, a high pressure pipe" ;
        <https://spec.industrialontologies.org/ontology/core/meta/AnnotationVocabulary/isPrimitive> "false"^^xsd:boolean ;
        <https://spec.industrialontologies.org/ontology/core/meta/AnnotationVocabulary/maturity> <https://spec.industrialontologies.org/ontology/core/meta/AnnotationVocabulary/Released> ;
        <https://spec.industrialontologies.org/ontology/core/meta/AnnotationVocabulary/semiFormalNaturalLanguageAxioms> "An LIS: Realizeable Entity that is realizrd in some OMPD: Hazard Realization Process" ;
        <https://spec.industrialontologies.org/ontology/core/meta/AnnotationVocabulary/subjectMatterExpertExplanation> "source or situation with a potential to cause injury and ill health (ISO 45001:2018)" .


###  http://spec.equonto.org/ontology/maintenance-procedure/static-procedure-ontology#HazardInMaintenanceProcess
:HazardInMaintenanceProcess rdf:type owl:Class ;
                            owl:equivalentClass [ owl:intersectionOf ( :Hazard
                                                                       [ rdf:type owl:Restriction ;
                                                                         owl:onProperty <http://rds.posccaesar.org/ontology/lis14/rdl/realizedIn> ;
                                                                         owl:someValuesFrom [ owl:intersectionOf ( :HazardRealizationProcess
                                                                                                                   [ rdf:type owl:Restriction ;
                                                                                                                     owl:onProperty <http://rds.posccaesar.org/ontology/lis14/rdl/occursRelativeTo> ;
                                                                                                                     owl:someValuesFrom :MaintenanceProcess
                                                                                                                   ]
                                                                                                                 ) ;
                                                                                              rdf:type owl:Class
                                                                                            ]
                                                                       ]
                                                                     ) ;
                                                  rdf:type owl:Class
                                                ] ;
                            rdfs:subClassOf <http://rds.posccaesar.org/ontology/lis14/rdl/Disposition> ;
                            rdfs:comment "An OMPD: Hazard that is realized in some OMPD: Hazard Realization Process that occurs relative to some OMPD: Maintenance Process" ;
                            rdfs:label "HazardInMaintenanceProcess" ;
                            <https://spec.industrialontologies.org/ontology/core/meta/AnnotationVocabulary/example> "a confined space, working at heights, pressurised containers" ;
                            <https://spec.industrialontologies.org/ontology/core/meta/AnnotationVocabulary/isPrimitive> "false"^^xsd:boolean ;
                            <https://spec.industrialontologies.org/ontology/core/meta/AnnotationVocabulary/maturity> <https://spec.industrialontologies.org/ontology/core/meta/AnnotationVocabulary/Released> ;
                            <https://spec.industrialontologies.org/ontology/core/meta/AnnotationVocabulary/semiFormalNaturalLanguageAxioms> "An OMPD: Hazard that is realized in some OMPD: Hazard Realization Process that occurs relative to some OMPD: Maintenance Process" ;
                            <https://spec.industrialontologies.org/ontology/core/meta/AnnotationVocabulary/subjectMatterExpertExplanation> "source or situation with a potential to cause injury and ill health identified in a maintenance procedure (ISO 45001:2018)" .


###  http://spec.equonto.org/ontology/maintenance-procedure/static-procedure-ontology#HazardRealizationProcess
:HazardRealizationProcess rdf:type owl:Class ;
                          rdfs:subClassOf <http://rds.posccaesar.org/ontology/lis14/rdl/Activity> ;
                          rdfs:comment "An LIS: Activity that has the potential to cause harm" ;
                          rdfs:label "HazardRealizationProcess" ;
                          <https://spec.industrialontologies.org/ontology/core/meta/AnnotationVocabulary/example> "a slip on the oil, the lifting of the too-heavy object, the unconstrained release of pressure" ;
                          <https://spec.industrialontologies.org/ontology/core/meta/AnnotationVocabulary/isPrimitive> "true"^^xsd:boolean ;
                          <https://spec.industrialontologies.org/ontology/core/meta/AnnotationVocabulary/maturity> <https://spec.industrialontologies.org/ontology/core/meta/AnnotationVocabulary/Released> ;
                          <https://spec.industrialontologies.org/ontology/core/meta/AnnotationVocabulary/semiFormalNaturalLanguageAxioms> "An LIS: Activity that has the potential to cause harm" ;
                          <https://spec.industrialontologies.org/ontology/core/meta/AnnotationVocabulary/subjectMatterExpertExplanation> "the process in which the hazard is realised and harm occurs" .


###  http://spec.equonto.org/ontology/maintenance-procedure/static-procedure-ontology#Image
:Image rdf:type owl:Class ;
       rdfs:subClassOf :MediaObject ;
       rdfs:comment "A LIS:MediaObject of type \"Image\"" ;
       rdfs:label "Image" ;
       <https://spec.industrialontologies.org/ontology/core/meta/AnnotationVocabulary/example> "an image of a pump, a schemaitc diagram (that can be accessed while viewing a procedure via a link)" ;
       <https://spec.industrialontologies.org/ontology/core/meta/AnnotationVocabulary/isPrimitive> "true"^^xsd:boolean ;
       <https://spec.industrialontologies.org/ontology/core/meta/AnnotationVocabulary/maturity> <https://spec.industrialontologies.org/ontology/core/meta/AnnotationVocabulary/Released> ;
       <https://spec.industrialontologies.org/ontology/core/meta/AnnotationVocabulary/semiFormalNaturalLanguageAxioms> "A LIS:MediaObject of type \"Image\"" .


###  http://spec.equonto.org/ontology/maintenance-procedure/static-procedure-ontology#InstructionalMaintenanceTaskDescription
:InstructionalMaintenanceTaskDescription rdf:type owl:Class ;
                                         owl:equivalentClass [ owl:intersectionOf ( :MaintenanceTaskDescription
                                                                                    [ rdf:type owl:Restriction ;
                                                                                      owl:onProperty :isInstructional ;
                                                                                      owl:hasValue "true"^^xsd:boolean
                                                                                    ]
                                                                                  ) ;
                                                               rdf:type owl:Class
                                                             ] ;
                                         rdfs:subClassOf :MaintenanceTaskDescription ;
                                         rdfs:comment "A LIS:MaintenanceTaskDescription that describes the actions necessary to execute a maintenance task" ;
                                         rdfs:label "InstructionalMaintenanceTaskDescription" ;
                                         <https://spec.industrialontologies.org/ontology/core/meta/AnnotationVocabulary/example> "\"unscrew the six bolts on the engine\"" ;
                                         <https://spec.industrialontologies.org/ontology/core/meta/AnnotationVocabulary/isPrimitive> "true"^^xsd:boolean ;
                                         <https://spec.industrialontologies.org/ontology/core/meta/AnnotationVocabulary/maturity> <https://spec.industrialontologies.org/ontology/core/meta/AnnotationVocabulary/Released> ;
                                         <https://spec.industrialontologies.org/ontology/core/meta/AnnotationVocabulary/semiFormalNaturalLanguageAxioms> "A LIS:MaintenanceTaskDescription that describes the actions necessary to execute a maintenance task" ;
                                         <https://spec.industrialontologies.org/ontology/core/meta/AnnotationVocabulary/subjectMatterExpertExplanation> "the part of a document containing instructions  for performing a maintenance task" .


###  http://spec.equonto.org/ontology/maintenance-procedure/static-procedure-ontology#ItemInMaintenanceProcess
:ItemInMaintenanceProcess rdf:type owl:Class ;
                          rdfs:subClassOf <http://rds.posccaesar.org/ontology/lis14/rdl/PhysicalObject> ;
                          rdfs:comment "A LIS:PhysicalObject that partiticipates in some OMPD:MaintenanceProcess" ;
                          rdfs:label "ItemInMaintenanceProcess" ;
                          <https://spec.industrialontologies.org/ontology/core/meta/AnnotationVocabulary/isPrimitive> "false"^^xsd:boolean ;
                          <https://spec.industrialontologies.org/ontology/core/meta/AnnotationVocabulary/maturity> <https://spec.industrialontologies.org/ontology/core/meta/AnnotationVocabulary/Released> ;
                          <https://spec.industrialontologies.org/ontology/core/meta/AnnotationVocabulary/semiFormalNaturalLanguageAxioms> "A LIS:PhysicalObject that partiticipates in some OMPD:MaintenanceProcess" ;
                          <https://spec.industrialontologies.org/ontology/core/meta/AnnotationVocabulary/subjectMatterExpertExplanation> "a physical object that is involved in some part of a maintenance task" .


###  http://spec.equonto.org/ontology/maintenance-procedure/static-procedure-ontology#MaintainableItem
:MaintainableItem rdf:type owl:Class ;
                  owl:equivalentClass [ owl:intersectionOf ( <http://rds.posccaesar.org/ontology/lis14/rdl/PhysicalObject>
                                                             [ rdf:type owl:Restriction ;
                                                               owl:onProperty <http://rds.posccaesar.org/ontology/lis14/rdl/hasRole> ;
                                                               owl:someValuesFrom :MaintainableItemRole
                                                             ]
                                                           ) ;
                                        rdf:type owl:Class
                                      ] ;
                  rdfs:subClassOf <http://rds.posccaesar.org/ontology/lis14/rdl/PhysicalObject> ;
                  rdfs:comment "An LIS:PhysicalObject hat has a role OMPD:MaintainableItemRole" ;
                  rdfs:label "MaintainableItem" ;
                  <https://spec.industrialontologies.org/ontology/core/meta/AnnotationVocabulary/adaptedFrom> "https://spec.industrialontologies.org/iof/ontology/core/Core/MaintainableMaterialItem" ;
                  <https://spec.industrialontologies.org/ontology/core/meta/AnnotationVocabulary/example> "a mechanical seal, a motor, tyres on a car" ;
                  <https://spec.industrialontologies.org/ontology/core/meta/AnnotationVocabulary/isPrimitive> "false"^^xsd:boolean ;
                  <https://spec.industrialontologies.org/ontology/core/meta/AnnotationVocabulary/maturity> <https://spec.industrialontologies.org/ontology/core/meta/AnnotationVocabulary/Released> ;
